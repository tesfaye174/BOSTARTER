{
    "name": "bostarter/backend",
    "description": "Backend ottimizzato per la piattaforma BOSTARTER",
    "type": "project",
    "version": "2.0.0",
    "require": {
        "php": ">=7.4"
    },
    "require-dev": {
        "php": ">=7.4"
    },
    "autoload": {
        "psr-4": {
            "BOSTARTER\\Models\\": "models/",
            "BOSTARTER\\Services\\": "services/",
            "BOSTARTER\\Utils\\": "utils/",
            "BOSTARTER\\Config\\": "config/"
        },
        "files": [
            "config/app_config.php"
        ]
    },
    "scripts": {
        "test": "echo 'No tests configured yet'",
        "install-dev": "echo 'Development environment ready'"
    },
    "authors": [
        {
            "name": "BOSTARTER Team",
            "email": "team@bostarter.com",
            "role": "Developer"
        }
    ],
    "license": "MIT",
    "minimum-stability": "stable",
    "prefer-stable": true,
    "config": {
        "optimize-autoloader": true,
        "classmap-authoritative": true
    }
}
